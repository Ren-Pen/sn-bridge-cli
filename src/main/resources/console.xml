<?xml version="1.0" encoding="UTF-8" ?>
<console xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.bioelectronic.top/cli http://www.bioelectronic.top/schema/bridge-cli.xsd"
        xmlns="http://www.bioelectronic.top/cli"
        prefix="system">
    <command bean="name:command:help" method="exec" name="help" description="显示帮助列表">
        <arguments>
            <argument name="debug" description="调试选项" simplify="d"/>
            <argument name="page" description="显示页数" simplify="p"/>
        </arguments>
    </command>
    <command bean="name:command:pluginLoad" method="exec" name="plugin" description="插件管理">
        <arguments>
            <argument name="load" simplify="l" description="加载">
                <excludes>
                    <exclude>unload</exclude>
                </excludes>
            </argument>
            <argument name="unload" simplify="u" description="卸载"/>
        </arguments>
    </command>


</console>
